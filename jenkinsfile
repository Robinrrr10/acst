pipeline {
    agent any
        stages {
            stage('Start'){
                steps {
                    echo "Starting the pipeline"
                }
            }
            stage('Repository') {
                steps {
                    echo "start for git repository download"
                }
            }
            stage('testing') {
                steps {
                    echo "Start testings"
                }
            }
            stage('Deployment') {
                steps {
                    echo "Start deployment"
                }
            }
            stage('End'){
                steps {
                    echo "Ending"
                }
            }

        }

}